<h1> this is new gear page</h1>


<%= form_with model: @gear do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :price_per_day %>
  <%= f.number_field :price_per_day %>
  <%= f.label :availability %>
  <%= f.check_box :availability %>
  <%= f.submit %>
<% end %>
